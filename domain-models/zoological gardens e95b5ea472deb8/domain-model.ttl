@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

<zoologist> a owl:Class, rdfs:Class;
    rdfs:label "Zoologist"@en;
    rdfs:comment "A specialised kind of staff member of a zoological garden."@en;
    rdfs:subClassOf <staff-member>.
<assigned-to-as-zoologist> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <zoologist>;
    rdfs:range <enclosure>.
<species> a owl:Class, rdfs:Class;
    rdfs:label "Species"@en;
    rdfs:comment "Species are used to classify animals, each possessing unique biological and ecological characteristics."@en.
<species-common-name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <species>.
<iucn-status> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <species>.
<resident-of> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <species>;
    rdfs:range <enclosure>.
<reproductive-age> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <species>.
<species-name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <species>.
<natural-breeding-time> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <species>.
<gestation-period> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <species>.
<climate-conditions> a owl:Class, rdfs:Class;
    rdfs:label "Climate Conditions"@en;
    rdfs:comment "Climate conditions are a blend of various controlled environmental factors including temperature, humidity, lighting, and ventilation. These conditions are essential for mimicking the natural habitat of animals as closely as possible, which not only ensures their physical health but also supports their psychological well-being by encouraging natural behaviors and physiological functions."@en.
<humidity-level> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <climate-conditions>.
<temperature> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <climate-conditions>.
<light-cycles> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <climate-conditions>.
<ultraviolet-light> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <climate-conditions>.
<ventilation> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <climate-conditions>.
<oviparous-species> a owl:Class, rdfs:Class;
    rdfs:label "Oviparous Species"@en;
    rdfs:comment "Species that laid eggs."@en;
    rdfs:subClassOf <species>.
<number-of-laid-eggs> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <oviparous-species>.
<caretaker> a owl:Class, rdfs:Class;
    rdfs:label "Caretaker"@en;
    rdfs:comment "A specialised kind of staff member of a zoological garden."@en;
    rdfs:subClassOf <staff-member>.
<assigned-as-caretaker> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <caretaker>;
    rdfs:range <enclosure>.
<veterinarian> a owl:Class, rdfs:Class;
    rdfs:label "Veterinarian"@en;
    rdfs:comment "A specialised kind of staff member of a zoological garden."@en;
    rdfs:subClassOf <staff-member>.
<responsible-for> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <veterinarian>;
    rdfs:range <species>.
<animal> a owl:Class, rdfs:Class;
    rdfs:label "Animal"@en;
    rdfs:comment "An animal is the central entity in a zoological garden."@en.
<has-specified-diet> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <animal>;
    rdfs:range <diet>.
<classified-by> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <animal>;
    rdfs:range <species>.
<animal-name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <animal>.
<reintroduction-conservation-program> a owl:Class, rdfs:Class;
    rdfs:label "Reintroduction Conservation Program"@en;
    rdfs:comment "A conservation program that reintroduces specific animals."@en.
<is-part-of-program> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <reintroduction-conservation-program>;
    rdfs:range <animal>.
<diet> a owl:Class, rdfs:Class;
    rdfs:label "Diet"@en;
    rdfs:comment "The diet of a particular animal in a zoological setting is a fundamental aspect of its care, impacting its health, behavior, and reproduction."@en.
<diet-type> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <diet>.
<nutrients> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <diet>.
<feeding-schedule> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <diet>.
<feeding-technique> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <diet>.
<conservation-program> a owl:Class, rdfs:Class;
    rdfs:label "Conservation Program"@en;
    rdfs:comment "Conservation programs a zoo participates in are initiatives aimed at preserving biodiversity through in-situ and ex-situ strategies. These programs often collaborate with global conservation entities and are pivotal in research, breeding programs, and reintroduction efforts."@en.
<country> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <conservation-program>.
<geographic-region> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <conservation-program>.
<supports> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <conservation-program>;
    rdfs:range <species>.
<administrative-personnel> a owl:Class, rdfs:Class;
    rdfs:label "Administrative Personnel"@en;
    rdfs:comment "A specialised kind of staff member of a zoological garden."@en;
    rdfs:subClassOf <staff-member>.
<zoological-garden> a owl:Class, rdfs:Class;
    rdfs:label "Zoological Garden"@en;
    rdfs:comment "A zoological garden is a complex entity where the interplay of biological diversity, human interaction, and environmental management coalesce into a singular institution aimed at conservation, education, and research."@en.
<provides> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <zoological-garden>;
    rdfs:range <enclosure>.
<participates-in> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <zoological-garden>;
    rdfs:range <conservation-program>.
<has-member> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <zoological-garden>;
    rdfs:range <staff-member>.
<staff-member> a owl:Class, rdfs:Class;
    rdfs:label "Staff member"@en;
    rdfs:comment "Staff members are integral to operation of zoological gardens"@en.
<permanent-address> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <staff-member>.
<birth-date> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <staff-member>.
<expertise> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <staff-member>.
<name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <staff-member>.
<enclosure> a owl:Class, rdfs:Class;
    rdfs:label "Enclosure"@en;
    rdfs:comment "A specialized habitat designed to mimic the natural environments of the resident species and for the ethical display and conservation of animals. "@en.
<enclosure-type> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <enclosure>.
<enclosure-size> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <enclosure>.
<has-climate-conditions> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <enclosure>;
    rdfs:range <climate-conditions>.
<vegetation> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <enclosure>.
