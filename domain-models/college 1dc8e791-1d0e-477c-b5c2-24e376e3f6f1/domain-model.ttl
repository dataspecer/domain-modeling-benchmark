@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

<body> a owl:Class, rdfs:Class;
    rdfs:label "Body"@en.
<internal-evaluation-board> a owl:Class, rdfs:Class;
    rdfs:label "Internal Evaluation Board"@en;
    rdfs:subClassOf <public-college-body>.
<has-internal-evaluation-board-member> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has internal evaluation board member"@en;
    rdfs:comment "Specifies a member of the internal evaluation board. The chairperson of the academic senate of a public college shall be a member of the internal evaluation board. The other members of the board shall be appointed by the rector, one third of whom shall be appointed on the proposal of the academic board and one third on the proposal of the academic senate, of whom one member shall always be appointed from among the students of the public college concerned."@en;
    rdfs:domain <internal-evaluation-board>;
    rdfs:range <natural-person>.
<has-internal-evaluation-board-chairperson> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has internal evaluation board chairperson"@en;
    rdfs:domain <internal-evaluation-board>;
    rdfs:range <rector>.
<has-internal-evaluation-board-vice-chairperson> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has internal evaluation board vice-chairperson"@en;
    rdfs:comment "The vice-chairperson of the board shall be appointed by the rector from among the academic staff of the public college who are professors or associate professors of the college."@en;
    rdfs:domain <internal-evaluation-board>;
    rdfs:range <academic-staff>.
<form-of-study> a owl:Class, rdfs:Class;
    rdfs:label "Form of Study"@en.
<academic-senate-elections> a owl:Class, rdfs:Class;
    rdfs:label "Academic Senate Elections"@en;
    rdfs:comment "Elections to the academic senate shall be direct, with a secret ballot."@en.
<elections-to> a rdf:Property, owl:ObjectProperty;
    rdfs:label "elections to"@en;
    rdfs:domain <academic-senate-elections>;
    rdfs:range <academic-senate>.
<state-doctoral-examination> a owl:Class, rdfs:Class;
    rdfs:label "State Doctoral Examination"@en.
<bachelor-study-programme> a owl:Class, rdfs:Class;
    rdfs:label "Bachelor Study Programme"@en;
    rdfs:comment "The Bachelor's degree programme is aimed at preparing students for the profession and for study in a Master's degree programme. The standard period of study, including work experience, is a minimum of three and a maximum of four years. Graduates of bachelor's degree programmes are awarded the academic degree of 'Bachelor' (abbreviated as 'B.Sc.' before their name) and, in the field of arts, the academic degree of 'Bachelor of Arts' (abbreviated as 'B.A.' before their name)."@en;
    rdfs:subClassOf <study-programme>.
<bachelor-profile-description> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "bachelor-profile-description"@en;
    rdfs:comment "The description of the profile of the graduate of the study programme."@en;
    rdfs:domain <bachelor-study-programme>.
<bachelor-state-final-examination> a owl:Class, rdfs:Class;
    rdfs:label "Bachelor State Final Examination"@en;
    rdfs:comment "The final examination by which a bachelor study programme is duly completed. It usually includes the defense of the bachelor's thesis."@en.
<includes-bachelor-thesis-defense> a rdf:Property, owl:ObjectProperty;
    rdfs:label "includes bachelor thesis defense"@en;
    rdfs:domain <bachelor-state-final-examination>;
    rdfs:range <bachelor-thesis-defense>.
<faculty> a owl:Class, rdfs:Class;
    rdfs:label "Faculty"@en;
    rdfs:comment "A faculty implements at least one accredited study programme and carries out creative activities."@en.
<implements-lifelong-learning-programme> a rdf:Property, owl:ObjectProperty;
    rdfs:label "implements lifelong learning programme"@en;
    rdfs:comment "A college implements accredited lifelong learning programmes."@en;
    rdfs:domain <faculty>;
    rdfs:range <lifelong-learning-programme>.
<implements-study-programme> a rdf:Property, owl:ObjectProperty;
    rdfs:label "implements study programme"@en;
    rdfs:comment "A college implements accredited study programmes."@en;
    rdfs:domain <faculty>;
    rdfs:range <study-programme>.
<employee> a owl:Class, rdfs:Class;
    rdfs:label "Employee"@en;
    rdfs:subClassOf <natural-person>.
<employed-at> a rdf:Property, owl:ObjectProperty;
    rdfs:label "employed at"@en;
    rdfs:domain <employee>;
    rdfs:range <college>.
<doctoral-study-programme> a owl:Class, rdfs:Class;
    rdfs:label "Doctoral Study Programme"@en;
    rdfs:comment "The doctoral study programme focuses on scientific research and independent creative activity in the field of research or development or on independent theoretical and creative activity in the field of art. The standard period of study shall be at least three and not more than four years. Graduates of doctoral programmes shall be awarded the academic degree of 'doctor' (abbreviated 'Ph.D.' after their name)."@en;
    rdfs:subClassOf <study-programme>.
<natural-person> a owl:Class, rdfs:Class;
    rdfs:label "Natural Person"@en.
<subject-board> a owl:Class, rdfs:Class;
    rdfs:label "Subject Board"@en;
    rdfs:comment "A subject board established in accordance with the internal regulations of the public college thereof which has accredited the relevant study programme."@en.
<public-college-body> a owl:Class, rdfs:Class;
    rdfs:label "Public College Body"@en;
    rdfs:subClassOf <body>.
<individual-study-plan> a owl:Class, rdfs:Class;
    rdfs:label "Individual Study Plan"@en.
<state-owned-college> a owl:Class, rdfs:Class;
    rdfs:label "State-owned College"@en;
    rdfs:subClassOf <college>.
<academic-community-member> a owl:Class, rdfs:Class;
    rdfs:label "Academic Community Member"@en.
<is-member-of-academic-community> a rdf:Property, owl:ObjectProperty;
    rdfs:label "is member of academic community"@en;
    rdfs:domain <academic-community-member>;
    rdfs:range <academic-community>.
<military-college> a owl:Class, rdfs:Class;
    rdfs:label "Military College"@en;
    rdfs:subClassOf <state-owned-college>.
<public-college> a owl:Class, rdfs:Class;
    rdfs:label "Public College"@en;
    rdfs:subClassOf <college>.
<legal-person> a owl:Class, rdfs:Class;
    rdfs:label "Legal Person"@en.
<participates-in-activities-of> a rdf:Property, owl:ObjectProperty;
    rdfs:label "participates in activities of"@en;
    rdfs:comment "Other legal persons engaged in the educational and creative activities may also participate in such activities of a college."@en;
    rdfs:domain <legal-person>;
    rdfs:range <college>.
<lifelong-learning-programme> a owl:Class, rdfs:Class;
    rdfs:label "Lifelong Learning Programme"@en.
<other-professional> a owl:Class, rdfs:Class;
    rdfs:label "Other Professional"@en;
    rdfs:subClassOf <natural-person>.
<participates-in-teaching> a rdf:Property, owl:ObjectProperty;
    rdfs:label "participates in teaching"@en;
    rdfs:comment "Other professionals may also participate in teaching on the basis of agreements on work performed outside the employment relationship."@en;
    rdfs:domain <other-professional>;
    rdfs:range <college>.
<bursar> a owl:Class, rdfs:Class;
    rdfs:label "Bursar"@en;
    rdfs:comment "The bursar of the public college."@en;
    rdfs:subClassOf <public-college-body>.
<examination> a owl:Class, rdfs:Class;
    rdfs:label "Examination"@en;
    rdfs:comment "An examination tests the studentâ€™s knowledge or his ability; the basic requirements for the examination are in accordance with the curriculum provided in the description of the subject and published in electronic form."@en;
    rdfs:subClassOf <subject-completion-assessment>.
<has-colloquium-prerequisite> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has colloquium prerequisite"@en;
    rdfs:domain <examination>;
    rdfs:range <colloquium>.
<has-course-credit-prerequisite> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has course credit prerequisite"@en;
    rdfs:domain <examination>;
    rdfs:range <course-credit>.
<form-of-examination> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "form of examination"@en;
    rdfs:comment "The forms of examination are oral, written, practical, or combined. Oral examinations and the oral component of combined examinations are open to members of the academic community of the college; for reasons of capacity, the presence of the members of the academic community may be limited"@en;
    rdfs:domain <examination>.
<has-course-test-prerequisite> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has course test prerequisite"@en;
    rdfs:domain <examination>;
    rdfs:range <course-test>.
<graded-course-credit> a owl:Class, rdfs:Class;
    rdfs:label "Graded Course Credit"@en;
    rdfs:comment "In contrast to course credit, a graded course credit denotes, in the same way as an examination, how well the student fulfilled the requirements for being granted the credit."@en;
    rdfs:subClassOf <subject-completion-assessment>.
<subject> a owl:Class, rdfs:Class;
    rdfs:label "Subject"@en;
    rdfs:comment "A subject of study at a college."@en.
<completion-is-considered-as-completion> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completion is considered as completion"@en;
    rdfs:comment "The completion of a subject is considered as the completion of another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<completion-has-completion-prerequisite> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completion has completion prerequisite"@en;
    rdfs:comment "The completion of a given subject has as a prerequisite the completion of another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<form-of-implementation> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "form of implementation"@en;
    rdfs:comment "The subject may be implemented in a full-time, distance learning, or combined form."@en;
    rdfs:domain <subject>.
<registration-excluded-by-simultaneous-registration> a rdf:Property, owl:ObjectProperty;
    rdfs:label "registration excluded by simultaneous registration"@en;
    rdfs:comment "The registration for a given subject is excluded by the simultaneous registration for another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<number-of-assigned-credits> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "number of assigned credits"@en;
    rdfs:domain <subject>.
<has-completion-prerequisite> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has prerequisite"@en;
    rdfs:comment "The registration for a given subject has as a prerequisite the completion of another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<number-of-semesters> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "number of semesters"@en;
    rdfs:comment "A subject may take one or two semesters in the same academic year."@en;
    rdfs:domain <subject>.
<form-of-teaching> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "form of teaching"@en;
    rdfs:comment "The subject is taught in particular in the form of lecture, tutorial, seminar, proseminar, course, practical training, internship, special course, laboratory class, field trip, fieldwork, individual work, or consultation."@en;
    rdfs:domain <subject>.
<is-part-of-curriculum> a rdf:Property, owl:ObjectProperty;
    rdfs:label "is part of curriculum"@en;
    rdfs:domain <subject>;
    rdfs:range <curriculum>.
<stipulates-elective-subjects> a rdf:Property, owl:ObjectProperty;
    rdfs:label "stipulates elective subjects"@en;
    rdfs:domain <subject>;
    rdfs:range <elective-subjects-group>.
<taught-within> a rdf:Property, owl:ObjectProperty;
    rdfs:label "taught within"@en;
    rdfs:domain <subject>;
    rdfs:range <study-programme>.
<completion-is-excluded-by-previous-completion> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completion is excluded by previous completion"@en;
    rdfs:comment "The completion of a given subject is excluded by the previous completion of another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<stipulates-compulsory-subject> a rdf:Property, owl:ObjectProperty;
    rdfs:label "stipulates compulsory subject"@en;
    rdfs:domain <subject>;
    rdfs:range <curriculum>.
<registration-has-simultaneous-registration-prerequisite> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has simultaneous registration prerequisite"@en;
    rdfs:comment "The registration for a given subject has as a prerequisite simultaneous registration for another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<registration-is-excluded-by-previous-completion> a rdf:Property, owl:ObjectProperty;
    rdfs:label "registration is excluded by previous completion"@en;
    rdfs:comment "The registration for a given subject is excluded by the previous completion of another subject."@en;
    rdfs:domain <subject>;
    rdfs:range <subject>.
<academic-community> a owl:Class, rdfs:Class;
    rdfs:label "Academic Community"@en;
    rdfs:comment "An academic community of a college is made up of its academic staff and students."@en.
<private-college> a owl:Class, rdfs:Class;
    rdfs:label "Private College"@en;
    rdfs:subClassOf <college>.
<master-studies> a owl:Class, rdfs:Class;
    rdfs:label "Master Studies"@en;
    rdfs:comment "The studies of a student enrolled in the Masters's degree study programme."@en.
<completed-by-master-state-final-examination> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completed by master state final examination"@en;
    rdfs:domain <master-studies>;
    rdfs:range <master-state-final-examination>.
<master-studies-of-a-student> a rdf:Property, owl:ObjectProperty;
    rdfs:label "master studies of a student"@en;
    rdfs:domain <master-studies>;
    rdfs:range <student>.
<studies-in-master-study-programme> a rdf:Property, owl:ObjectProperty;
    rdfs:label "studies in master study programme"@en;
    rdfs:domain <master-studies>;
    rdfs:range <master-study-programme>.
<non-university-college> a owl:Class, rdfs:Class;
    rdfs:label "Non-university College"@en;
    rdfs:comment "A non-university college may carry out bachelor's degree programmes and may also carry out master's degree programmes and related creative activities. A non-university college shall not be divided into faculties."@en;
    rdfs:subClassOf <college>.
<master-study-programme> a owl:Class, rdfs:Class;
    rdfs:label "Master Study Programme"@en;
    rdfs:comment "The Master's degree programme is aimed at acquiring theoretical and practical knowledge based on the current state of scientific knowledge, research and development, at mastering their application and at developing skills for creative activity; in the field of art, it is focused on demanding artistic training and the development of talent. The Master's degree programme is a follow-up to the Bachelor's degree programme; the standard duration of this programme is at least one and not more than three years. The following academic degrees are awarded to graduates of master's degree programmes:\n(a) in the fields of economics, technical sciences and technology, agriculture, forestry and military science, 'engineer' (abbreviated 'Ing.' before the name),\n(b) in the field of architecture, 'engineer architect' (abbreviated 'Ing. arch.' before the name),\n(c) in general medicine, 'Doctor of Medicine' (abbreviated 'M.D.' before the name),\n(d) in the field of dentistry, 'Doctor of Dental Medicine' (abbreviated MDDr. before the name),\n(e) in the field of veterinary medicine and veterinary hygiene, 'Doctor of Veterinary Medicine' (abbreviated 'MVDr.' before the name),\n(f) in the arts, 'Master of Arts' (abbreviated 'MgA.' before the name),\n(g) in other fields, 'Master' (abbreviated 'Mgr.' before the name)."@en;
    rdfs:subClassOf <study-programme>.
<master-profile-description> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "master profile description"@en;
    rdfs:comment "The description of the profile of the graduate of the study programme."@en;
    rdfs:domain <master-study-programme>.
<scientific-council> a owl:Class, rdfs:Class;
    rdfs:label "Scientific Council"@en;
    rdfs:subClassOf <public-college-body>.
<has-scientific-council-member> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has scientific council member"@en;
    rdfs:comment "Specifies a member of the scientific council of the public college. A member is a prominent representative of the fields in which the college carries out educational and creative activities."@en;
    rdfs:domain <scientific-council>;
    rdfs:range <natural-person>.
<has-scientific-council-chairperson> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has scientific council chairperson"@en;
    rdfs:domain <scientific-council>;
    rdfs:range <rector>.
<study-programme> a owl:Class, rdfs:Class;
    rdfs:label "Study Programme"@en.
<belongs-to-field> a rdf:Property, owl:ObjectProperty;
    rdfs:label "belongs to field"@en;
    rdfs:domain <study-programme>;
    rdfs:range <education-field>.
<programme-name> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "programme name"@en;
    rdfs:domain <study-programme>.
<has-study-programme-guarantor> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has guarantor"@en;
    rdfs:comment "For study programmes carried out by a faculty, the faculty appoints and dismisses a study programme guarantor from among its academic staff according to the rules laid down in the internal regulations of the college. The guarantor may only be an academic staff who meets the conditions laid down in the standards for accreditation of the study programme for the proper performance of activities. Only an associate professor, a full professor or an extraordinary professor who is an academic staff member of the college concerned may be the guarantor of a master's degree programme and a doctoral degree programme. The degree programme guarantor shall, in particular, coordinate the content of the degree programme, supervise the quality of its implementation, evaluate the degree programme and develop it."@en;
    rdfs:domain <study-programme>;
    rdfs:range <academic-staff>.
<standard-period-of-study> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "standard period of study"@en;
    rdfs:comment "The study programme must have a standard period of study with an average study load expressed in academic years."@en;
    rdfs:domain <study-programme>.
<has-curriculum> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has curriculum"@en;
    rdfs:domain <study-programme>;
    rdfs:range <curriculum>.
<programme-objectives> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "programme objectives"@en;
    rdfs:comment "The objectives of the study in the study programme."@en;
    rdfs:domain <study-programme>.
<programme-type> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "programme type"@en;
    rdfs:domain <study-programme>.
<awarded-degree> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "awarded degree"@en;
    rdfs:comment "The degree awarded to graduates of the study programme."@en;
    rdfs:domain <study-programme>.
<remote-subject-completion-assessment> a owl:Class, rdfs:Class;
    rdfs:label "Remote Subject Completion Assessment"@en;
    rdfs:comment "If justified or purposeful, it is possible to perform the assessment of study of a subject remotely, either in part or in full, using information and communication technologies, providing that it is possible to verify the identity of the student and to ensure compliance with the rules set for the given assessment of study of the subject."@en;
    rdfs:subClassOf <subject-completion-assessment>.
<elective-subjects-group> a owl:Class, rdfs:Class;
    rdfs:label "Elective Subjects Group"@en.
<course-credit> a owl:Class, rdfs:Class;
    rdfs:label "Course Credit"@en;
    rdfs:subClassOf <subject-completion-assessment>.
<doctoral-studies> a owl:Class, rdfs:Class;
    rdfs:label "Doctoral Studies"@en.
<doctoral-studies-of-a-student> a rdf:Property, owl:ObjectProperty;
    rdfs:label "doctoral studies of a student"@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <student>.
<carried-out-according-to> a rdf:Property, owl:ObjectProperty;
    rdfs:label "carried out according to"@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <individual-study-plan>.
<guided-by> a rdf:Property, owl:ObjectProperty;
    rdfs:label "guided by"@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <supervisor>.
<monitored-by> a rdf:Property, owl:ObjectProperty;
    rdfs:label "monitored by"@en;
    rdfs:comment "A subject board that monitors and evaluates the studies in a doctoral programme."@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <subject-board>.
<completed-by-state-doctoral-examination> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completed by state doctoral examination"@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <state-doctoral-examination>.
<studies-in-doctoral-study-programme> a rdf:Property, owl:ObjectProperty;
    rdfs:label "studies in doctoral study programme"@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <doctoral-study-programme>.
<completed-by-dissertation-defense> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completed by dissertation defense"@en;
    rdfs:domain <doctoral-studies>;
    rdfs:range <dissertation-defense>.
<student> a owl:Class, rdfs:Class;
    rdfs:label "Student"@en;
    rdfs:comment "A student is a natural person who is enrolled in a study program."@en;
    rdfs:subClassOf <natural-person>, <academic-community-member>.
<enrolled-in> a rdf:Property, owl:ObjectProperty;
    rdfs:label "enrolled in"@en;
    rdfs:domain <student>;
    rdfs:range <study-programme>.
<registers-to-subject> a rdf:Property, owl:ObjectProperty;
    rdfs:label "registers to subject"@en;
    rdfs:domain <student>;
    rdfs:range <subject>.
<is-student-member-of-academic-community> a rdf:Property, owl:ObjectProperty;
    rdfs:label "is student member of academic community"@en;
    rdfs:domain <student>;
    rdfs:range <academic-community>.
<elected-as-student-senate-member-in> a rdf:Property, owl:ObjectProperty;
    rdfs:label "elected as student senate member in"@en;
    rdfs:domain <student>;
    rdfs:range <academic-senate-elections>.
<course-test> a owl:Class, rdfs:Class;
    rdfs:label "Course Test"@en;
    rdfs:subClassOf <subject-completion-assessment>.
<distance-learning-form-of-study> a owl:Class, rdfs:Class;
    rdfs:label "Distance Learning Form of Study"@en;
    rdfs:comment "For the distance learning form of study, the curriculum is created within the framework of the programme of study based on the assumption that study is offered in the distance form of instruction, and that teachers usually perform the role of consultant."@en;
    rdfs:subClassOf <form-of-study>.
<academic-staff> a owl:Class, rdfs:Class;
    rdfs:label "Academic Staff"@en;
    rdfs:comment "Academic staff are those full professors, associate professors, extraordinary professors, assistant professors, assistant lecturers, lecturers and scientific, research and development staff who are employees of the college carrying out both teaching and creative activities in an employment relationship according to the agreed type of work. Academic staff are obliged to uphold the reputation of the college."@en;
    rdfs:subClassOf <employee>, <natural-person>, <academic-community-member>.
<elected-as-staff-senate-member-in> a rdf:Property, owl:ObjectProperty;
    rdfs:label "elected as staff senate member in"@en;
    rdfs:domain <academic-staff>;
    rdfs:range <academic-senate-elections>.
<is-staff-member-of-academic-commumity> a rdf:Property, owl:ObjectProperty;
    rdfs:label "is staff member of academic commumity"@en;
    rdfs:domain <academic-staff>;
    rdfs:range <academic-community>.
<curriculum> a owl:Class, rdfs:Class;
    rdfs:label "Curriculum"@en;
    rdfs:comment "The curriculum determines the sequence in time of the courses within the study programme. It also determines the form of study of the individual subjects in the curriculum and the method of verification of the learning outcomes achieved in the subjects."@en.
<determines-temporal-sequence> a rdf:Property, owl:ObjectProperty;
    rdfs:label "determines temporal sequence"@en;
    rdfs:domain <curriculum>;
    rdfs:range <subject>.
<determines-form-of-study> a rdf:Property, owl:ObjectProperty;
    rdfs:label "determines form of study"@en;
    rdfs:comment "The form of study determined by the curriculum."@en;
    rdfs:domain <curriculum>;
    rdfs:range <form-of-study>.
<contains-elective-group> a rdf:Property, owl:ObjectProperty;
    rdfs:label "contains elective group"@en;
    rdfs:comment "A curriculum contains groups of elective courses."@en;
    rdfs:domain <curriculum>;
    rdfs:range <elective-subjects-group>.
<education-field> a owl:Class, rdfs:Class;
    rdfs:label "Education Field"@en;
    rdfs:comment "A field of education is a defined section of higher education within which study programmes of close or related content are prepared, approved and implemented, reflecting a common theoretical and methodological basis of the given field of education. There are the following areas of education: Architecture and Urban Planning, Security, Biology, Ecology and Environment, Transport, Economics, Electrical Engineering, Energy, Pharmacy, Philosophy, Religious Studies and Theology, Physics, Historical Sciences, Chemistry, Computer Science, Cybernetics, Forestry and Wood Science, Mathematics, Media and Communication Studies, Non-teaching Pedagogy, Political Science, Food Science, Law, Psychology, Social Work, Sociology, Civil Engineering, Engineering, Technology and Materials, Physical Education and Sport; Kinanthropology, Mining and Mineral Processing, Teaching, Arts, Arts and Cultural Sciences, Earth Sciences, Veterinary Medicine, Veterinary Hygiene, General Medicine and Dentistry."@en.
<colloquium> a owl:Class, rdfs:Class;
    rdfs:label "Colloquium"@en;
    rdfs:subClassOf <subject-completion-assessment>.
<bachelor-thesis-defense> a owl:Class, rdfs:Class;
    rdfs:label "Bachelor Thesis Defense"@en.
<self-governing-academic-body> a owl:Class, rdfs:Class;
    rdfs:label "Self-governing Academic Body"@en;
    rdfs:subClassOf <public-college-body>.
<supervisor> a owl:Class, rdfs:Class;
    rdfs:label "Supervisor"@en;
    rdfs:comment "A supervisor is an academic staff who is a scientific expert in the field."@en;
    rdfs:subClassOf <academic-staff>.
<dissertation> a owl:Class, rdfs:Class;
    rdfs:label "Dissertation"@en;
    rdfs:comment "The dissertation must contain original and published results or results accepted for publication and it shall demonstrate the ability and readiness for independent research or development activities or independent theoretical and creative artistic activity."@en.
<college> a owl:Class, rdfs:Class;
    rdfs:label "College"@en;
    rdfs:comment "Colleges, as the highest element in the education system, are the ultimate centres of learning, independent knowledge and creative activity and play a key role in the scientific, cultural, social and economic development of society."@en;
    rdfs:subClassOf <legal-person>.
<structured-into> a rdf:Property, owl:ObjectProperty;
    rdfs:label "structured into"@en;
    rdfs:comment "A college is structured into its bodies."@en;
    rdfs:domain <college>;
    rdfs:range <body>.
<has-academic-community> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has academic community"@en;
    rdfs:domain <college>;
    rdfs:range <academic-community>.
<divided-into> a rdf:Property, owl:ObjectProperty;
    rdfs:label "divided into"@en;
    rdfs:comment "A public college is divided into faculties."@en;
    rdfs:domain <college>;
    rdfs:range <faculty>.
<diploma-thesis-defense> a owl:Class, rdfs:Class;
    rdfs:label "Master Thesis Defense"@en.
<rector> a owl:Class, rdfs:Class;
    rdfs:label "Rector"@en;
    rdfs:comment "The rector is the head of the public college. It is a member of the academic community of the public college elected in rector's elections. The rector acts and decides on the affairs of the college, unless otherwise provided by law. In cases where a special regulation provides for the competence of the statutory body, the rector shall exercise it. The term of office of the rector shall be four years. The office of rector may be held by the same person at the same public college for a maximum of two consecutive terms."@en;
    rdfs:subClassOf <public-college-body>, <academic-community-member>.
<is-head-of> a rdf:Property, owl:ObjectProperty;
    rdfs:label "is head of"@en;
    rdfs:comment "The rector is the head of the public college."@en;
    rdfs:domain <rector>;
    rdfs:range <college>.
<board-of-trustees> a owl:Class, rdfs:Class;
    rdfs:label "Board of Trustees"@en;
    rdfs:subClassOf <public-college-body>.
<has-board-of-trustees-member> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has board of trustees member"@en;
    rdfs:comment "The board of trustees of a public college shall have at least nine members, the number of which must always be divisible by three. The members of the board of trustees of a public college shall be appointed by the minister in consultation with the rector, so that it includes adequate representation, in particular, of representatives of public life, professional chambers, employers' organisations or other persons or bodies carrying out, supporting or benefiting from the educational or creative activities of public colleges or their results, representatives of local and state government and alumni of the institution. Members of the board may not be employees of the college."@en;
    rdfs:domain <board-of-trustees>;
    rdfs:range <natural-person>.
<combined-form-of-study> a owl:Class, rdfs:Class;
    rdfs:label "Combined Form of Study"@en;
    rdfs:comment "For the combined form of study, the curriculum is created within the programme of study based on the assumption that study takes a form which employs the principles of both full-time and distance learning study."@en;
    rdfs:subClassOf <form-of-study>.
<master-state-final-examination> a owl:Class, rdfs:Class;
    rdfs:label "Master State Final Examination"@en;
    rdfs:comment "The final examination by which a master study programme is duly completed. It includes the defense of the diploma thesis."@en.
<includes-diploma-thesis-defense> a rdf:Property, owl:ObjectProperty;
    rdfs:label "includes diploma thesis defense"@en;
    rdfs:domain <master-state-final-examination>;
    rdfs:range <diploma-thesis-defense>.
<university> a owl:Class, rdfs:Class;
    rdfs:label "University"@en;
    rdfs:comment "A university college (or shortly a university) may carry out all types of study programmes and related scientific and research, development and innovation, artistic or other creative activities (or shortly creative activities)."@en;
    rdfs:subClassOf <college>.
<combined-study-programme> a owl:Class, rdfs:Class;
    rdfs:label "Combined Study Programme"@en;
    rdfs:comment "A study programme is a combined programme of study if the state examinations or, where appropriate, the dissertation defence test profiling knowledge or skills in the core subject areas belonging to more than one field of education."@en;
    rdfs:subClassOf <study-programme>.
<police-college> a owl:Class, rdfs:Class;
    rdfs:label "Police College"@en;
    rdfs:subClassOf <state-owned-college>.
<academic-senate> a owl:Class, rdfs:Class;
    rdfs:label "Academic Senate"@en;
    rdfs:comment "The academic senate of a public college is its self-governing representative academic body. It shall have at least eleven members, of whom at least one third and no more than one half shall be students. The members of the academic senate of a public college shall be elected from among the members of the academic community of the public college."@en;
    rdfs:subClassOf <self-governing-academic-body>.
<has-chairperson> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has chairperson"@en;
    rdfs:comment "The academic senate shall have a chairperson, who shall be one of its academic staff members."@en;
    rdfs:domain <academic-senate>;
    rdfs:range <academic-community-member>.
<has-student-member> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has student member"@en;
    rdfs:comment "The members of the academic senate of a public college are the members of the academic community of the public college who were elected in elections to the academic senate. The membership of the academic senate of a public college is incompatible with the office of rector, vice-rector, bursar, dean, vice-dean, faculty secretary, and director of the college. The term of office of individual members of the academic senate of a public college shall be for a maximum of three years."@en;
    rdfs:domain <academic-senate>;
    rdfs:range <student>.
<has-staff-member> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has staff member"@en;
    rdfs:comment "The members of the academic senate of a public college are the members of the academic community of the public college who were elected in elections to the academic senate. The membership of the academic senate of a public college is incompatible with the office of rector, vice-rector, bursar, dean, vice-dean, faculty secretary, and director of the college. The term of office of individual members of the academic senate of a public college shall be for a maximum of three years."@en;
    rdfs:domain <academic-senate>;
    rdfs:range <academic-staff>.
<subject-completion-assessment> a owl:Class, rdfs:Class;
    rdfs:label "Subject Completion Assessment"@en;
    rdfs:comment "Assessment of completion of the study of a subject means the verification of completion of the subject by the student."@en.
<assesses-completion-of> a rdf:Property, owl:ObjectProperty;
    rdfs:label "assesses completion of"@en;
    rdfs:domain <subject-completion-assessment>;
    rdfs:range <subject>.
<results-grade> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "results grade"@en;
    rdfs:comment "The results of colloquium, course credit, and course test are graded as \"pass\"-\"fail\" or \"credited\"-\"not credited\". Successful completion of these assessments of study of the subject means the classification \"pass\" or \"credited\". The results of examination and graded course credit are graded as \"excellent\", \"very good\", \"good\", and \"fail\". Successful completion of these assessments of study of the subject means the classification \"excellent\", \"very good\", and \"good\"."@en;
    rdfs:domain <subject-completion-assessment>.
<assessment-date> a rdf:Property, owl:DatatypeProperty;
    rdfs:label "assessment date"@en;
    rdfs:domain <subject-completion-assessment>.
<assesses-completion-by> a rdf:Property, owl:ObjectProperty;
    rdfs:label "assesses completion by"@en;
    rdfs:domain <subject-completion-assessment>;
    rdfs:range <student>.
<carried-out-by-examiner> a rdf:Property, owl:ObjectProperty;
    rdfs:label "carried out by examiner"@en;
    rdfs:comment "The assessment of the study is carried out by an examiner, who is always an academic member of staff."@en;
    rdfs:domain <subject-completion-assessment>;
    rdfs:range <academic-staff>.
<full-time-form-of-study> a owl:Class, rdfs:Class;
    rdfs:label "Full-time Form of Study"@en;
    rdfs:comment "For the full-time form of study, the curriculum is created within the framework of a programme of study in such a way that the largest part of instruction takes the form of lectures, tutorials, seminars, courses, practical training, laboratory classes, consultation, and other similar forms of instruction, which are held according to a regular schedule, which will be usually a weekly schedule; they enable students to acquire knowledge directly."@en;
    rdfs:subClassOf <form-of-study>.
<disciplinary-board> a owl:Class, rdfs:Class;
    rdfs:label "Disciplinary Board"@en;
    rdfs:subClassOf <public-college-body>.
<has-disciplinary-board-chairperson> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has disciplinary board chairperson"@en;
    rdfs:comment "The disciplinary board of a public college shall elect its chairperson from among its members."@en;
    rdfs:domain <disciplinary-board>;
    rdfs:range <academic-community-member>.
<has-disciplinary-board-member> a rdf:Property, owl:ObjectProperty;
    rdfs:label "has disciplinary board member"@en;
    rdfs:comment "The members of the disciplinary board of a public college are appointed by the rector from among the members of the academic community of the college. Half of the members of the disciplinary board of a public college shall be students."@en;
    rdfs:domain <disciplinary-board>;
    rdfs:range <academic-community-member>.
<single-field-study-programme> a owl:Class, rdfs:Class;
    rdfs:label "Single Field Study Programme"@en;
    rdfs:comment "A study programme belongs to only one field of education if the state examinations or the dissertation defence verify profiling knowledge or skills in the basic subject areas belonging to one field of education."@en;
    rdfs:subClassOf <study-programme>.
<bachelor-studies> a owl:Class, rdfs:Class;
    rdfs:label "Bachelor Studies"@en;
    rdfs:comment "The studies of a student enrolled in the Bachelor's degree study programme."@en.
<bachelor-studies-of-student> a rdf:Property, owl:ObjectProperty;
    rdfs:label "bachelor studies of a student"@en;
    rdfs:domain <bachelor-studies>;
    rdfs:range <student>.
<studies-in-bachelor-study-programme> a rdf:Property, owl:ObjectProperty;
    rdfs:label "studies in bachelor study programme"@en;
    rdfs:domain <bachelor-studies>;
    rdfs:range <bachelor-study-programme>.
<completed-by-bachelor-state-final-examination> a rdf:Property, owl:ObjectProperty;
    rdfs:label "completed by bachelor state final examination"@en;
    rdfs:domain <bachelor-studies>;
    rdfs:range <bachelor-state-final-examination>.
<dissertation-defense> a owl:Class, rdfs:Class;
    rdfs:label "Dissertation Defense"@en;
    rdfs:comment "A public defense of the disertation."@en.
<is-defense-of-dissertation> a rdf:Property, owl:ObjectProperty;
    rdfs:label "is defense of dissertation"@en;
    rdfs:domain <dissertation-defense>;
    rdfs:range <dissertation>.
