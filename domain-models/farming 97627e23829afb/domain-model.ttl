@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

<farmer> a owl:Class, rdfs:Class;
    rdfs:label "Farmer"@en;
    rdfs:comment "An individual who engages in the activity of agriculture, growing and harvesting crops."@en.
<owns-machinery> a rdf:Property, owl:ObjectProperty;
    rdfs:label "owns machinery"@en;
    rdfs:comment "A farmer can own one or more machinery. The machinery owned by the farmer reflects the capacity for mechanized farming."@en.
<conducts-planting> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <farmer>;
    rdfs:range <planting>.
<name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <farmer>.
<farms-on> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <farmer>;
    rdfs:range <field>.
<age> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <farmer>.
<fruit> a owl:Class, rdfs:Class;
    rdfs:label "Fruit"@en;
    rdfs:comment "Plants cultivated for producing their seed-associated fleshy structures that typically are sweet or sour and edible in the raw state, such as apples, bananas, grapes, lemons, oranges, and strawberries."@en;
    rdfs:subClassOf <crop>.
<vegetable> a owl:Class, rdfs:Class;
    rdfs:label "Vegetable"@en;
    rdfs:comment "Plants cultivated for their parts consumable by humans or other animals as food."@en;
    rdfs:subClassOf <crop>.
<planting> a owl:Class, rdfs:Class;
    rdfs:label "Planting"@en;
    rdfs:comment "Represents the agricultural activity of planting crops in a specific field during a specific year. It captures the details of the planting process."@en.
<results-in-harvest> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <planting>;
    rdfs:range <harvest>.
<performed-on> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <planting>;
    rdfs:range <field>.
<involves-crop> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <planting>;
    rdfs:range <crop>.
<planting-date> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <planting>.
<planter> a owl:Class, rdfs:Class;
    rdfs:label "Planter"@en;
    rdfs:comment "Machinery designed to plant seeds efficiently and accurately, crucial for optimal crop distribution and growth."@en;
    rdfs:subClassOf <machinery>.
<crop> a owl:Class, rdfs:Class;
    rdfs:label "Crop"@en;
    rdfs:comment "Plants grown in large amounts and cultivated by farmers for profit or subsistence, including cereals, oil seeds, vegetables, and fruits."@en.
<cultivated-variety> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <crop>.
<field> a owl:Class, rdfs:Class;
    rdfs:label "Field"@en;
    rdfs:comment "An area of land used for growing crops that can be but does not have to be surrounded by a fence."@en.
<irrigation-availability> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <field>.
<location> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <field>.
<soil-ph> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <field>.
<size> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <field>.
<soil-type> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <field>.
<topography> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <field>.
<harvest> a owl:Class, rdfs:Class;
    rdfs:label "Harvest"@en;
    rdfs:comment "The production that is obtained from crops. It is quantified and evaluated in terms of quantity and quality. It represents the final output of the agricultural cycle."@en.
<quantity> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <harvest>.
<harvest-date> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <harvest>.
<quality> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <harvest>.
<harvested-by> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <harvest>;
    rdfs:range <farmer>.
<tractor> a owl:Class, rdfs:Class;
    rdfs:label "Tractor"@en;
    rdfs:comment "Versatile piece of machinery essential for multiple farming tasks, from soil preparation to crop maintenance."@en;
    rdfs:subClassOf <machinery>.
<seed-plant> a owl:Class, rdfs:Class;
    rdfs:label "Seed Plant"@en;
    rdfs:comment "Plants grown for oil production from seeds, like sunflower and soybean, significant for both food and industrial uses."@en;
    rdfs:subClassOf <crop>.
<cereal> a owl:Class, rdfs:Class;
    rdfs:label "Cereal"@en;
    rdfs:comment "A cereal is a plant cultivated for its edible grain."@en;
    rdfs:subClassOf <crop>.
<machinery> a owl:Class, rdfs:Class;
    rdfs:label "Machinery"@en;
    rdfs:comment "Mechanical tools and equipment used in farming, crucial for tasks such as plowing, seeding, spraying, and harvesting."@en.
<used-for-field-preparation> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <machinery>;
    rdfs:range <planting>.
<year-of-manufacturing> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <machinery>.
<model> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <machinery>.
<used-for-planting> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <machinery>;
    rdfs:range <planting>.
<used-for-harvesting> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <machinery>;
    rdfs:range <harvest>.
<harvester> a owl:Class, rdfs:Class;
    rdfs:label "Harvester"@en;
    rdfs:comment "Specialized machinery that is used for harvesting. It also includes machinery that combines harvesting tasks such as reaping, threshing, and winnowing into a single operation."@en;
    rdfs:subClassOf <machinery>.
<sprayer> a owl:Class, rdfs:Class;
    rdfs:label "Sprayer"@en;
    rdfs:comment "Used for the precise application of liquids like pesticides and fertilizers, vital for crop protection and growth enhancement."@en;
    rdfs:subClassOf <machinery>.
