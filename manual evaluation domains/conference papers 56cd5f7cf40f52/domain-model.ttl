@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

<conference> a owl:Class, rdfs:Class;
    rdfs:label "Conference"@en;
    rdfs:comment "An event where research papers are presented and discussed."@en.
<location> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <conference>.
<year> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <conference>.
<conference-name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <conference>.
<review> a owl:Class, rdfs:Class;
    rdfs:label "Review"@en;
    rdfs:comment "An evaluation or critique of a research paper by a reviewer."@en.
<reviews> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <review>;
    rdfs:range <submission>.
<author> a owl:Class, rdfs:Class;
    rdfs:label "Author"@en;
    rdfs:comment "A person who has significantly contributed to the creation of a research paper."@en;
    rdfs:subClassOf <person>.
<has-corresponding-author> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <author>;
    rdfs:range <research-paper>.
<affiliation> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <author>.
<research-paper> a owl:Class, rdfs:Class;
    rdfs:label "Research Paper"@en;
    rdfs:comment "A document presenting original research findings."@en.
<written-by> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <research-paper>;
    rdfs:range <author>.
<abstract> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <research-paper>.
<title> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <research-paper>.
<publication-year> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <research-paper>.
<presented-at> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <research-paper>;
    rdfs:range <conference>.
<has-submission> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <research-paper>;
    rdfs:range <submission>.
<person> a owl:Class, rdfs:Class;
    rdfs:label "Person"@en;
    rdfs:comment "An individual who is involved in the conference reviewing process."@en.
<name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <person>.
<email> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <person>.
<reviewer> a owl:Class, rdfs:Class;
    rdfs:label "Reviewer"@en;
    rdfs:comment " A person responsible for evaluating the quality and relevance of a research paper submission."@en;
    rdfs:subClassOf <person>.
<expertise> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <reviewer>.
<writes> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <reviewer>;
    rdfs:range <review>.
<submission> a owl:Class, rdfs:Class;
    rdfs:label "Submission"@en;
    rdfs:comment "The act of submitting a research paper for consideration at a conference. It represents the research paper in the conference review process."@en.
<is-resubmission-of> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <submission>;
    rdfs:range <submission>.
<submitted-to> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <submission>;
    rdfs:range <conference>.
<submission-date> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <submission>.
<status> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <submission>.
<reviewed-by> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <submission>;
    rdfs:range <reviewer>.
