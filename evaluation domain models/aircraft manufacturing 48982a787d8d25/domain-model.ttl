@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

<aircraft-model> a owl:Class, rdfs:Class;
    rdfs:label "Aircraft Model"@en;
    rdfs:comment "Represents different models of aircraft."@en.
<aircraft-model-code> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <aircraft-model>.
<aircraft-model-description> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <aircraft-model>.
<incorporates> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <aircraft-model>;
    rdfs:range <part-model>.
<customer> a owl:Class, rdfs:Class;
    rdfs:label "Customer"@en;
    rdfs:comment "Represents the clients who purchase the finished aircraft."@en;
    rdfs:subClassOf <party>.
<purchases> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <customer>;
    rdfs:range <aircraft>.
<customer-name> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <customer>.
<part-model> a owl:Class, rdfs:Class;
    rdfs:label "Part Model"@en;
    rdfs:comment "Represents the design model for individual parts."@en.
<part-model-number> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <part-model>.
<part-model-description> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <part-model>.
<supply> a owl:Class, rdfs:Class;
    rdfs:label "Supply"@en;
    rdfs:comment "Encompasses the parts, required for aircraft manufacturing, supplied by a given supplier."@en.
<supplied-for> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <supply>;
    rdfs:range <aircraft>.
<includes> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <supply>;
    rdfs:range <part>.
<part> a owl:Class, rdfs:Class;
    rdfs:label "Part"@en;
    rdfs:comment "Represents individual components used in the construction of an aircraft."@en.
<part-description> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <part>.
<part-serial-number> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <part>.
<designed-as-part-model> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <part>;
    rdfs:range <part-model>.
<supplier> a owl:Class, rdfs:Class;
    rdfs:label "Supplier"@en;
    rdfs:comment "Represents entities that provide parts for aircraft manufacturing."@en;
    rdfs:subClassOf <party>.
<supplies> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <supplier>;
    rdfs:range <supply>.
<party> a owl:Class, rdfs:Class;
    rdfs:label "Party"@en;
    rdfs:comment "Represents a generic subject involved in the aircraft manufacturing process."@en.
<party-code> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <party>.
<aircraft> a owl:Class, rdfs:Class;
    rdfs:label "Aircraft"@en;
    rdfs:comment "Represents the final manufactured product."@en.
<aircraft-serial-number> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <aircraft>.
<manufacturing-date> a rdf:Property, owl:DatatypeProperty;
    rdfs:domain <aircraft>.
<consists-of> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <aircraft>;
    rdfs:range <part>.
<designed-as> a rdf:Property, owl:ObjectProperty;
    rdfs:domain <aircraft>;
    rdfs:range <aircraft-model>.
